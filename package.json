{
  "name": "simple-icons-font",
  "version": "1.9.25",
  "description": "Icon font generated from https://simpleicons.org",
  "homepage": "https://www.simpleicons.org",
  "keywords": [
    "svg",
    "icons",
    "font",
    "typeface"
  ],
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/simple-icons/simple-icons-font.git"
  },
  "bugs": {
    "url": "https://github.com/simple-icons/simple-icons-font/issues"
  },
  "author": "Simple Icons Collaborators",
  "license": "CC0-1.0",
  "scripts": {
    "build": "npm run build:font && npm run build:css",
    "build:css": "node scripts/buildCss.js",
    "build:font": "node scripts/buildFont.js",
    "build:testpage": "npm run build && node scripts/buildTestpage.js",
    "prepublish": "npm run build",
    "test": "npm run build:testpage",
    "version": "node scripts/version"
  },
  "devDependencies": {
    "icon-font-buildr": "^1.3.2",
    "pug": "^2.0.3",
    "simple-icons": "1.9.25"
  }
}
